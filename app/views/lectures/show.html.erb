<p>
  <strong>Name:</strong>
  <%= @lecture.name %>
</p>

<p>
  <strong>Enrollment key:</strong>
  <%= @lecture.enrollment_key %>
</p>

<p>
  <strong>Status:</strong>
  <%= @lecture.status%>
</p>

<h2>Polls</h2>
<% for poll in @lecture.polls %>
	<ul>
		<li><%= poll.title %></li>
	</ul>
<% end %>

<%= link_to 'Edit', edit_lecture_path(@lecture) %> |
<%= link_to 'New Poll', new_lecture_poll_path(@lecture) %> |
<%= link_to 'Back', lectures_path %>
